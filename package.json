{
  "name": "bestofjs-webui",
  "version": "0.6.3",
  "description": "bestof.js.org front-end web application",
  "main": "",
  "engines": {
    "node": ">=6.2.1"
  },
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "cross-env NODE_ENV=test babel-tape-runner test/**/*Spec.js | faucet",
    "test-debug": "babel-tape-runner test/**/HomeSpec.js",
    "clean": "rimraf www/index.html && rimraf www/build",
    "build": "npm run build-html && npm run build-js",
    "start": "cross-env NODE_ENV=development node config/devserver.js",
    "build-js": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --progress",
    "gh-pages": "node scripts/deploy-gh-pages/",
    "firebase": "firebase deploy --project project-2939241904987176880",
    "surge": "surge --domain bestofjs.surge.sh --project ./www/",
    "deploy-all": "npm run build && npm run gh-pages",
    "daily": "npm run build-html && npm run build-html-hof && npm run deploy-html",
    "build-html": "babel-node scripts/build/",
    "build-html-hof": "babel-node scripts/build/build-hof.js",
    "deploy-html": "babel-node scripts/deploy-html/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrambeau/bestofjs-webui.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrambeau/bestofjs-webui/issues"
  },
  "homepage": "http://bestof.js.org",
  "dependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "0.26.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-minifier": "^3.2.2",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "marked": "^0.3.5",
    "node-fetch": "^1.3.3",
    "notie": "^3.9.4",
    "numeral": "^1.5.3",
    "postcss-loader": "^1.1.1",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-scroll": "0.4.1",
    "react-select": "1.0.0-rc.2",
    "redux": "^3.0.4",
    "redux-form": "6.2.0",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^2.1.0",
    "slideout": "^0.1.11",
    "style-loader": "0.13.1",
    "stylus-loader": "2.3.1",
    "stylus": "0.54.5",
    "url-loader": "^0.5.7",
    "vague-time": "^2.4.0",
    "webpack": "1.13.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-register": "6.18.0",
    "babel-tape-runner": "2.0.1",
    "enzyme": "2.6.0",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "6.7.1",
    "eslint-plugin-standard": "^2.0.1",
    "faucet": "0.0.1",
    "gh-pages": "0.12.0",
    "jsdom": "^9.8.3",
    "react-addons-perf": "15.4.0",
    "react-addons-test-utils": "15.4.0",
    "react-hot-loader": "1.3.0",
    "sinon": "^1.17.3",
    "tape": "^4.2.2",
    "webpack-dev-server": "1.16.2",
    "why-did-you-update": "0.0.8"
  }
}
